    <script>

        $(function () {
            $.get("share/header.html", function (html) {
                $("header").html(html);

                //顽固搜索
                $(".finder-button").click(function () {
                    $(".finder-button .icon-ctrl-down").toggleClass("icon-rotate");
                    $(".finder-panel").toggleClass("active");
                });
            });

            $.get("share/footer.html", function (html) {
                $("footer").html(html);
            });

            $('.jqzoom').jqzoom({
                zoomType: 'standard',
                lens: true,
                preloadImages: false,
                alwaysOn: false,
            });

            $('#images-slider').carousel({
                interval: false
            });

            $('#info-tab a').click(function (e) {
                e.preventDefault()
                $(this).tab('show')
            })

            $("img.lazy").lazyload({
                effect: "fadeIn"
            });

            $(".tab-wrap").pin({
                containerSelector: ".info-right"
            })
        });
    </script>
